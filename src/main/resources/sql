CREATE TABLE `22NZuZPPMQ`.`processed` (
  `ID` INT NOT NULL AUTO_INCREMENT,
  `OFFER` VARCHAR(45) NULL,
  `EMAIL` VARCHAR(45) NULL,
  `IP` VARCHAR(45) NULL,
  `DATE` DATE NULL,
  `STATE` VARCHAR(45) NULL,
  `USER` VARCHAR(45) NULL,
  PRIMARY KEY (`ID`));


CREATE TABLE `22NZuZPPMQ`.`mails` (
  `ID` INT NOT NULL AUTO_INCREMENT,
  `EMAIL` VARCHAR(45) NULL,
  PRIMARY KEY (`ID`));
